{
  "brand": "RPG Chat",
  "default_theme": "dark",
  "default_hs_url": "https://chat.roleplaygateway.com",
  "default_is_url": "https://chat.roleplaygateway.com",
  "integrations_ui_url": "https://dimension.fabric.pub",
  "integrations_rest_url": "https://dimension.fabric.pub",
  "bug_report_endpoint_url": "https://chat.fabric.pub/bugs",
  "update_base_url": "https://chat.fabric.pub/releases",
  "features": {
    "feature_groups": "enable",
    "feature_pinning": "enable",
    "feature_presence": "enable",
    "feature_reactions": "enable",
    "feature_message_editing": "enable"
  },
  "disable_3pid_login": true,
  "disable_login_language_selector": true,
  "disable_guests": true,
  "disable_custom_urls": true,
  "roomDirectory": {
    "servers": [
      "fabric.pub",
      "roleplaygateway.com",
      "matrix.org"
    ]
  },
  "embeddedPages": {
    "homeUrl": "home.html"
  },
  "settingDefaults": {
    "language": "en_EN"
  },
  "branding": {
    "authHeaderLogoUrl": "themes/rpg/img/logos/roleplaygateway.png",
    "welcomeBackgroundUrl": "themes/rpg/img/backgrounds/roleplaygateway.jpg"
  }
}
